{
  "name": "@coeps/https-reverse-proxy",
  "version": "1.0.0",
  "description": "simple configurable https reverse proxy",
  "keywords": ["https", "reverse", "proxy", "config", "simple"],
  "main": "https-reverse-proxy.js",
  "bin": {
    "https-reverse-proxy": "https-reverse-proxy.js"
  },
  "scripts": {
    "start": "node https-reverse-proxy.js --config config.json",
    "pkg": "pkg --out-dir release --targets node10-macos-x64,node10-alpine-x64,node10-linux-x64,node10-win https-reverse-proxy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coeps-github/https-reverse-proxy.git"
  },
  "author": "coeps@coeps.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coeps-github/https-reverse-proxy/issues"
  },
  "homepage": "https://github.com/coeps-github/https-reverse-proxy#readme",
  "dependencies": {
    "debug": "4.1.1",
    "express": "4.17.3",
    "http-proxy": "1.17.0",
    "rc": "1.2.8"
  },
  "devDependencies": {
    "pkg": "4.4.0"
  }
}
